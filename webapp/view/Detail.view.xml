<mvc:View
    controllerName="vc.monitor.controller.Detail"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page
        id="page"
        title="Detail"
    >
        <content>
            <HBox
                id="_IDGenHBox1"
                alignContent="Center"
            >
                <GenericTile
                    id="_IDGenGenericTile1"
                    press="navigateForward1('Dashboard')"
                    class="sapUiSmallMarginBegin sapUiSmallMarginTop tileLayout"
                    header="Dashboard"
                />
                <GenericTile
                    id="_IDGenGenericTile2"
                    press="navigateForward2('Parametros')"
                    class="sapUiSmallMarginBegin sapUiSmallMarginTop tileLayout"
                    header="Parametros"
                />
            </HBox>
        </content>
    </Page>
</mvc:View>
